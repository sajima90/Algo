{
	"algorithme": {
		"prefix": [
			"Algo"
		],
		"body": [
			"Algo ${1:nom}",
			"Constantes:",
			"\n\t$2",
			"\nVariables:",
			"\n\t$3",
			"\nDebut",
			"\n\t$4",
			"\nFIN"
		],
		"description": "algorithme"
	},

	"selon": {
		"prefix": [
			"selon"
		],
		"body": [
			"selon ${1:operateur}",
			"\n\tcas $2 :",
			"\n\tcas $3 :",
			"\n\tautre cas : $4",
			"\nfselon"
		],
		"description": "selon"
	},
	"boucle pour": {
		"prefix": "pour",
		"body": [
			"pour ${1:i} de ${2:0} à ${3:N-1} faire",
			"\t$0",
			"fin_pour"
		],
		"description": "boucle pour"
	},
	"si...alors": {
		"prefix": "si-alors",
		"body": [
			"si ${1:conditon} alors",
			"\t$0",
			"fsi"
		],
		"description": "si...alors"
	},
	"si...alors...sinon": {
		"prefix": "si-sinon",
		"body": [
			"si ${1:conditon} alors",
			"\t$0",
			"sinon",
			"\t",
			"fsi"
		],
		"description": "si...alors...sinon"
	},
	"boucle tant que": {
		"prefix": "tq",
		"body": [
			"tq ${1:condition} faire",
			"\t$0",
			"ftq"
		],
		"description": "boucle tant que"
	},
	"boucle répéter": {
		"prefix": [
			"répéter"
		],
		"body": [
			"répéter",
			"\t$0",
			"jusqu'à  ${1:condition}"
		],
